{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "types": ["vitest/globals"],
    "baseUrl": ".",
    "paths": {
          "@/*": ["src/*"],
          "@innobridge/usermanagement": ["./node_modules/@innobridge/usermanagement/dist/index"]
    }
  },
  "include": ["src/**/*", "tsup.config.ts", "vitest.config.ts"]
}
